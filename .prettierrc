{
	"printWidth": 100,
	"tabWidth": 2,
	"useTabs": false,
	"semi": true,
	"singleQuote": false,
	"quoteProps": "consistent",
	"trailingComma": "all",
	"bracketSpacing": true,
	"arrowParens": "always",
	"htmlWhitespaceSensitivity": "ignore",
	"endOfLine": "lf",
	"embeddedLanguageFormatting": "auto",
	"bracketSameLine": false,
	"singleAttributePerLine": true
}